{
  "compilerOptions": {
    "incremental": true,
    "target": "es2020",
    "emitDecoratorMetadata": true,
    "module": "commonjs",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "strictNullChecks": false,
    "skipLibCheck": true,
    "experimentalDecorators": true
    // "baseUrl": "./src"
  },
  "include": ["src/**/*.ts"]
}
